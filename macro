Sub CONCORRENTES()
'
' CONCORRENTES Macro

'Parte 1
       

    'BASE DE CONCORRENTES

     Dim counter As Integer
     counter = 1
     Do While counter < 5
     counter = counter + 1


    Sheets("base geral").Select
    Range("A2:B2").Select
    Range(Selection, Selection.End(xlDown)).Select
    Selection.Copy
    Sheets("BASE PONTO EXTRA").Select
    Range("A2").Select
    Do
    If ActiveCell <> "" Then
    ActiveCell.Offset(1, 0).Select
    End If
    Loop Until ActiveCell = ""
    ActiveCell.Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
        Application.CutCopyMode = False



    Sheets("base geral").Select
    Range("E1").Select
    Selection.Copy
    Sheets("BASE PONTO EXTRA").Select
    Range("c2:c122").Select
    ActiveCell.Select
    Do
    If ActiveCell <> "" Then
    ActiveCell.Offset(1, 0).Select
    End If
    Loop Until ActiveCell = ""
    ActiveCell.Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
    :=False, Transpose:=False
    Application.CutCopyMode = False

    Application.CutCopyMode = False
    Selection.Copy
    ActiveCell.Offset(0, -1).Range("A1").Select
    Selection.End(xlDown).Select
    ActiveCell.Offset(0, 1).Range("A1").Select
    Range(Selection, Selection.End(xlUp)).Select
    ActiveCell.Activate
    ActiveSheet.Paste
    Application.CutCopyMode = False



    Sheets("base geral").Select
    Range("E2").Select
    Range(Selection, Selection.End(xlDown)).Select
